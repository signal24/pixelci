{
    "name": "@zyno-io/pixelci-cli",
    "private": true,
    "version": "0.1.0",
    "description": "",
    "type": "module",
    "main": "./dist/index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "dev": "APP_ENV=${APP_ENV:-development} npm-run-all clean -p dev:tsc dev:run",
        "dev:tsc": "tsc -w --preserveWatchOutput -p tsconfig.json",
        "dev:run": "sh -c 'while ! [ -f dist/index.js ]; do sleep 1; done; dotenv yarn start:debug ../api/tests/integration/fixtures'",
        "start": "node dist/index.js",
        "start:debug": "node --inspect=9210 --watch dist/index.js",
        "test": "tsc && node --experimental-test-module-mocks --test dist/*.spec.js",
        "format": "npx oxlint --fix --fix-suggestions --fix-dangerously && prettier --ignore-path ../../.prettierignore --write ."
    },
    "devDependencies": {
        "@faker-js/faker": "^10.3.0",
        "@types/node": "^25.2.3",
        "dotenv-cli": "^11.0.0",
        "npm-run-all": "^4.1.5",
        "typescript": "~5.9"
    },
    "keywords": [],
    "author": "",
    "license": "UNLICENSED",
    "dependencies": {
        "@deepkit/core": "^1.0.19",
        "@deepkit/type": "^1.0.19",
        "@deepkit/type-compiler": "^1.0.19"
    }
}
